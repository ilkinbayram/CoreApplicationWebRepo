<!--Breadcrumb Banner Area Start-->
<div class="container">
    <div class="row">
        <div class="col-md-12">
            @*<h1 class="text-center">@("ContactPageCaptionText.ViewLocalization".Translate())</h1>*@
            <div class="mapouter">
                <div class="gmap_canvas"><iframe class="gmap_iframe" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?width=600&amp;height=400&amp;hl=en&amp;q=Port Baku Residence&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"></iframe><a href="https://www.fnfgo.com/"></a></div>
                <style>
                    .mapouter {
                        left: 0 !important;
                        top: 0 !important;
                        position: relative;
                        text-align: right;
                        width: 100%;
                    }

                    .gmap_canvas {
                        overflow: hidden;
                        background: none !important;
                        width: 100%;
                    }

                    .gmap_iframe {
                        width: 100% !important;
                        height: 300px !important;
                        height: 500px !important;
                    }
                </style>
            </div>
            <div class="breadcrumb-bar">
                <ul style="top: 0 !important; margin-top: 0 !important;" class="breadcrumb">
                    <li>@Html.ActionLink("HomePageNavLink.ViewLocalization".Translate(), "TouchIndex", "Home", new { area = "Global" })</li>
                    <li>@("ContactPageCaptionText.ViewLocalization".Translate())</li>
                </ul>
            </div>
        </div>
    </div>
</div>
<!--End of Breadcrumb Banner Area-->